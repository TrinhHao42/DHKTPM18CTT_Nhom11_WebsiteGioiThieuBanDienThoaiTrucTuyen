{
  "name": "free-nextjs-admin-dashboard",
  "version": "2.0.2",
  "private": true,
  "scripts": {
    "dev": "next dev --port 3001",
    "dev:frontend": "next dev --port 3001",
    "dev:analytics": "cd analytics-backend && ANALYTICS_PORT=3001 tsx watch index.ts",
    "dev:all": "concurrently \"npm run dev:frontend\" \"npm run dev:analytics\"",
    "build": "next build",
    "build:frontend": "next build",
    "build:analytics": "cd analytics-backend && tsc -p tsconfig.build.json",
    "build:all": "npm run build:frontend && npm run build:analytics",
    "start": "next start",
    "start:frontend": "next start --port 3001",
    "start:analytics": "cd analytics-backend && PORT=3002 node dist/index.js",
    "analytics:start": "cd analytics-backend && PORT=3002 node dist/index.js",
    "start:all": "concurrently \"npm run start:frontend\" \"npm run start:analytics\"",
    "lint": "eslint .",
    "dev:tunnel": "next dev -H 0.0.0.0 -p 3000 & lt --port 3000 --subdomain enternal-rune-admin",
    "prisma:migrate": "cd analytics-backend && prisma migrate dev",
    "prisma:generate": "cd analytics-backend && prisma generate",
    "prisma:studio": "cd analytics-backend && prisma studio",
    "prisma:seed": "cd analytics-backend && tsx prisma/seed.ts",
    "prisma:reset": "cd analytics-backend && prisma migrate reset",
    "prisma:deploy": "cd analytics-backend && prisma migrate deploy",
    "docker:build": "docker build -t enternal-rune-app .",
    "docker:run": "docker run -p 3001:3001 -p 3002:3002 enternal-rune-app",
    "docker:up": "docker-compose up",
    "docker:up:build": "docker-compose up --build",
    "docker:up:detached": "docker-compose up -d",
    "docker:up:build:detached": "docker-compose up -d --build",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "docker:clean": "docker-compose down -v --remove-orphans && docker system prune -f"
  },
  "dependencies": {
    "@fullcalendar/core": "^6.1.15",
    "@fullcalendar/daygrid": "^6.1.15",
    "@fullcalendar/interaction": "^6.1.15",
    "@fullcalendar/list": "^6.1.15",
    "@fullcalendar/react": "^6.1.15",
    "@fullcalendar/timegrid": "^6.1.15",
    "@prisma/client": "^5.7.0",
    "@react-jvectormap/core": "^1.0.4",
    "@react-jvectormap/world": "^1.1.2",
    "@tailwindcss/forms": "^0.5.9",
    "@tailwindcss/postcss": "^4.0.9",
    "apexcharts": "^4.3.0",
    "autoprefixer": "^10.4.20",
    "concurrently": "^8.2.2",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "flatpickr": "^4.6.13",
    "localtunnel": "^2.0.2",
    "next": "16.0.3",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.7.1",
    "react": "19.2.0",
    "react-apexcharts": "^1.7.0",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-dom": "19.2.0",
    "react-dropzone": "^14.3.5",
    "swiper": "^11.2.0",
    "tailwind-merge": "^2.6.0"
  },
  "devDependencies": {
    "@svgr/webpack": "^8.1.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20",
    "@types/react": "19.2.4",
    "@types/react-dom": "19.2.3",
    "@types/react-transition-group": "^4.4.12",
    "eslint": "^9",
    "eslint-config-next": "16.0.3",
    "postcss": "^8",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.7.1",
    "prisma": "^5.7.0",
    "tailwindcss": "^4.0.0",
    "tsx": "^4.6.2",
    "typescript": "^5"
  },
  "overrides": {
    "@react-jvectormap/core": {
      "react": "^16.8.0 || ^17 || ^18 || ^19",
      "react-dom": "^16.8.0 || ^17 || ^18 || ^19"
    },
    "@react-jvectormap/world": {
      "react": "^16.8.0 || ^17 || ^18 || ^19",
      "react-dom": "^16.8.0 || ^17 || ^18 || ^19"
    },
    "@types/react": "19.2.4",
    "@types/react-dom": "19.2.3"
  }
}
